{
    "borderNone": true,
    "brackets": true,
    "colons": true,
    "colors": false,
    "commaSpace": true,
    "commentSpace": true,
    "cssLiteral": false,
    "depthLimit": false,
    "duplicates": false,
    "efficient": true,
    "emoji": false,
    "enforceBlockStyle": false,
    "enforceVarStyle": true,
    "extendPref": false,
    "globalDupe": false,
    "indentPref": 2,
    "indentSpaces": 2,
    "leadingZero": true,
    "maxWarnings": 10,
    "maxWarningsKill": false,
    "mixed": true,
    "namingConvention": "BEM",
    "namingConventionStrict": false,
    "parenSpace": false,
    "placeholders": false,
    "quotePref": "single",
    "semicolons": "never",
    "sortOrder": "alphabetical",
    "stackedProperties": true,
    "universal": true,
    "valid": true,
    "whitespace": true,
    "zeroUnits": true,
    "zIndexDuplicates": false,
    "zIndexNormalize": false
}
